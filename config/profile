set_proxy()
{
    if grep -qEi "(Microsoft|WSL)" /proc/version; then
        source $HOME/.config/my-profile/config/wsl_proxy.conf
    fi
}

set_script()
{
    source $HOME/.config/my-profile/config/script.conf
}

set_theme()
{
    source $HOME/.config/my-profile/config/bash-theme.conf
}

set_proxy
set_script
set_theme


